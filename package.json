{
  "name": "polychrome.p5",
  "version": "1.0.51",
  "description": "A colorful text painter",
  "main_orig": "index.html/sketch.js",
  "main": "src/index.js",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean": "rm -rf dist",
    "deploy": "gh-pages -t -d dist",
    "bump": "npm version patch",
    "publish": "npm run bump && DEPLOY_ENV=GH_PAGES npm run generate && npm run deploy",
    "lint:clean": "standard --fix",
    "lint": "standard"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelPaulukonis/polychrome.p5.git"
  },
  "keywords": [
    "p5",
    "text",
    "paint"
  ],
  "author": "Michael J. Paulukonis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelPaulukonis/polychrome.p5/issues"
  },
  "homepage": "https://github.com/MichaelPaulukonis/polychrome.p5#readme",
  "dependencies": {
    "@cyrilf/vue-dat-gui": "^0.3.0",
    "@nuxtjs/axios": "^5.6.0",
    "cheerio": "^1.0.0-rc.3",
    "dat.gui": "^0.7.6",
    "hotkeys-js": "^3.7.2",
    "keypress.js": "^2.1.5",
    "nuxt": "^2.10.2",
    "p5": "^0.7.3",
    "ramda": "^0.26.1",
    "vue-js-modal": "^1.3.31"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.0.3",
    "cypress": "^3.1.4",
    "eslint": "^6.5.1",
    "eslint-config-p5js": "^1.0.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "open": "^7.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "standard": "^11.0.1",
    "start-server-and-test": "^1.7.11"
  },
  "eslintConfigFoo": {
    "extends": "p5js"
  },
  "standard": {
    "ignore": [
      "**/dat.gui.js"
    ],
    "extends": [
      "p5js",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "env": {
      "cypress/globals": true
    },
    "globals": [
      "VERSION",
      "IMAGES"
    ]
  }
}
