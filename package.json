{
  "name": "polychrome.p5",
  "version": "1.0.64",
  "description": "A colorful text painter",
  "main_orig": "index.html/sketch.js",
  "main": "src/index.js",
  "engines": {
    "node": "14.16.1"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean": "rm -rf dist",
    "deploy": "gh-pages -t -d dist",
    "bump": "npm version patch",
    "publish": "npm run bump && DEPLOY_ENV=GH_PAGES npm run generate && npm run deploy",
    "lint:fix": "standard --fix",
    "lint": "standard",
    "open": "open https://michaelpaulukonis.github.io/polychrome.p5/"
  },
  "lint-staged": {
    "*.{js,vue}": "standard"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelPaulukonis/polychrome.p5.git"
  },
  "keywords": [
    "p5",
    "text",
    "paint"
  ],
  "author": "Michael J. Paulukonis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaelPaulukonis/polychrome.p5/issues"
  },
  "homepage": "https://github.com/MichaelPaulukonis/polychrome.p5#readme",
  "dependencies": {
    "@cyrilf/vue-dat-gui": "^0.3.0",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "cheerio": "^1.0.0-rc.3",
    "epic-spinners": "^1.1.0",
    "file-saver": "^2.0.5",
    "hotkeys-js": "^3.7.2",
    "keypress.js": "^2.1.5",
    "nuxt": "^2.10.2",
    "nuxt-webfontloader": "^1.1.0",
    "p5": "^0.10.2",
    "ramda": "^0.26.1",
    "vue-js-modal": "^1.3.31"
  },
  "devDependencies": {
    "@nuxtjs/eslint-module": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "cypress": "^3.1.4",
    "eslint": "^7.25.0",
    "eslint-config-p5js": "^1.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "node-sass": "^4.13.1",
    "open": "^7.0.0",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.1.1",
    "standard": "^16.0.3",
    "start-server-and-test": "^1.7.11"
